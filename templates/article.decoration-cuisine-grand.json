    "0ce687f7-c459-476c-be8f-093aec6781a4": {
      "type": "image-with-text",
      "blocks": {
        "template--18952696496475__0ce687f7-c459-476c-be8f-093aec6781a4-168414635562ea7de4-0": {
          "type": "heading",
